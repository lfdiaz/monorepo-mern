{
  "name": "root",
  "private": true,
  "devDependencies": {
    "lerna": "^3.20.2",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "concurrently": "^5.1.0"
  },
  "scripts": {
    "client": "cd packages/ui && yarn run startDev",
    "server": "cd packages/backend && yarn run startDev",
    "startDev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client\""
  }
}
